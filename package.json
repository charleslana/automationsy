{
  "name": "automationsy",
  "version": "1.0.0",
  "description": "Easy automated testing functions for beginners and advanced",
  "repository": "github:charleslana/automationsy",
  "homepage": "",
  "author": "Charles Lana",
  "license": "ISC",
  "keywords": [
    "automation",
    "automationsy",
    "test",
    "test automation",
    "puppeteer"
  ],
  "engines": {
    "node": ">=14"
  },
  "main": "src/AutomationSy",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "build": "tsc",
    "start": "npm run build && node dist/test/01.test.js",
    "test": "npm run build && node dist/test/$npm_config_name.test.js",
    "multiple": "npm run build && node dist/test/01.test.js && node dist/test/02.test.js"
  },
  "dependencies": {
    "puppeteer": "^19.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1"
  }
}
